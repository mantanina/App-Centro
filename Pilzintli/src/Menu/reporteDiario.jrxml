	<queryString>
		<![CDATA[SELECT
  consulta.id,
  consulta.hora,
  paciente.nombre,
  paciente.apellido_paterno,
  paciente.apellido_materno
FROM consulta
  INNER JOIN paciente
    ON consulta.paciente_id = paciente.id
  INNER JOIN especialista
    ON consulta.especialista_id = especialista.id
WHERE consulta.fecha = CURDATE()]]>
	</queryString>
